<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class 
        name="com.sgnbs.symbolbox.ecommerce.customer.po.Customer" 
        table="T_USER_INFO"
    >
        <id name="userid" column="USERID" type="java.lang.Integer">
            <generator class="assigned"/>
        </id>
        <property 
            name="username"
            column="USERNAME"
            update="true"
            insert="true"
            type="java.lang.String"
            not-null="true"
            unique="false"
            length="40"
        />
        <property 
            name="password"
            column="PASSWORD"
            update="true"
            insert="true"
            type="java.lang.String"
            not-null="true"
            unique="false"
            length="20"
        />
        <property 
            name="passwordtype"
            column="PASSWORDTYPE"
            update="true"
            insert="true"
            type="java.lang.Integer"
            not-null="true"
            unique="false"
            length="0"
        />
        <property 
            name="fullname"
            column="FULLNAME"
            update="true"
            insert="true"
            type="java.lang.String"
            not-null="false"
            unique="false"
            length="100"
        />
        <property 
            name="status"
            column="STATUS"
            update="true"
            insert="true"
            type="java.lang.Integer"
            not-null="true"
            unique="false"
            length="0"
        />
        <property 
            name="country"
            column="COUNTRY"
            update="true"
            insert="true"
            type="java.lang.String"
            not-null="false"
            unique="false"
            length="40"
        />
        <property 
            name="city"
            column="CITY"
            update="true"
            insert="true"
            type="java.lang.String"
            not-null="false"
            unique="false"
            length="40"
        />
        <property 
            name="phone"
            column="PHONE"
            update="true"
            insert="true"
            type="java.lang.String"
            not-null="false"
            unique="false"
            length="40"
        />
        <property 
            name="mobile"
            column="MOBILE"
            update="true"
            insert="true"
            type="java.lang.String"
            not-null="false"
            unique="false"
            length="40"
        />
        <property 
            name="email"
            column="EMAIL"
            update="true"
            insert="true"
            type="java.lang.String"
            not-null="false"
            unique="false"
            length="100"
        />
        <property 
            name="address"
            column="ADDRESS"
            update="true"
            insert="true"
            type="java.lang.String"
            not-null="false"
            unique="false"
            length="200"
        />
        <property 
            name="fax"
            column="FAX"
            update="true"
            insert="true"
            type="java.lang.String"
            not-null="false"
            unique="false"
            length="40"
        />
        <property 
            name="remark"
            column="REMARK"
            update="true"
            insert="true"
            type="java.lang.String"
            not-null="false"
            unique="false"
            length="400"
        />
        <property 
            name="createtime"
            column="CREATETIME"
            update="true"
            insert="true"
            type="java.util.Date"
            not-null="false"
            unique="false"
            length="0"
        />
        <property 
            name="updatetime"
            column="UPDATETIME"
            update="true"
            insert="true"
            type="java.util.Date"
            not-null="false"
            unique="false"
            length="0"
        />
        <property 
            name="modpwdtime"
            column="MODPWDTIME"
            update="true"
            insert="true"
            type="java.util.Date"
            not-null="false"
            unique="false"
            length="0"
        />
        <property 
            name="rsv1"
            column="RSV1"
            update="true"
            insert="true"
            type="java.lang.String"
            not-null="false"
            unique="false"
            length="40"
        />
        <property 
            name="rsv2"
            column="RSV2"
            update="true"
            insert="true"
            type="java.lang.String"
            not-null="false"
            unique="false"
            length="40"
        />
		<!-- One-To-Many attributes -->
		<!-- Many-To-One attributes -->
    </class>
</hibernate-mapping>

