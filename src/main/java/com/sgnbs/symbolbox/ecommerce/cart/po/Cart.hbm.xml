<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class 
        name="com.sgnbs.symbolbox.ecommerce.cart.po.Cart" 
        table="T_SHOPPONGCART_INFO"
    >
        <composite-id name="compID" class="com.sgnbs.symbolbox.ecommerce.cart.po.CartPK">
            <key-property name="userid" type="java.lang.Integer" column="USERID"/>
            <key-property name="appid" type="java.lang.Integer" column="APPID"/>
        </composite-id>
        <property 
            name="scode"
            column="SCODE"
            update="true"
            insert="true"
            type="java.lang.String"
            not-null="true"
            unique="false"
            length="100"
        />
        <property 
            name="quantity"
            column="QUANTITY"
            update="true"
            insert="true"
            type="java.lang.Integer"
            not-null="true"
            unique="false"
            length="0"
        />
        <property 
            name="confirmtime"
            column="CONFIRMTIME"
            update="true"
            insert="true"
            type="java.util.Date"
            not-null="true"
            unique="false"
            length="0"
        />
        <property 
            name="updatetime"
            column="UPDATETIME"
            update="true"
            insert="true"
            type="java.util.Date"
            not-null="false"
            unique="false"
            length="0"
        />
        <property 
            name="rsv1"
            column="RSV1"
            update="true"
            insert="true"
            type="java.lang.String"
            not-null="false"
            unique="false"
            length="40"
        />
        <property 
            name="rsv2"
            column="RSV2"
            update="true"
            insert="true"
            type="java.lang.String"
            not-null="false"
            unique="false"
            length="40"
        />
		<!-- One-To-Many attributes -->
		<!-- Many-To-One attributes -->
    </class>
</hibernate-mapping>

