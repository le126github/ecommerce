<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class 
        name="com.sgnbs.symbolbox.ecommerce.order.po.Order" 
        table="T_ORDER_INFO"
    >
        <id name="orderid" column="ORDERID" type="java.lang.Integer">
            <generator class="assigned"/>
        </id>
        <property 
            name="userid"
            column="USERID"
            update="true"
            insert="true"
            type="java.lang.Integer"
            not-null="true"
            unique="false"
            length="0"
        />
        <property 
            name="appids"
            column="APPIDS"
            update="true"
            insert="true"
            type="java.lang.String"
            not-null="true"
            unique="false"
            length="600"
        />
        <property 
            name="quantitys"
            column="QUANTITYS"
            update="true"
            insert="true"
            type="java.lang.String"
            not-null="true"
            unique="false"
            length="200"
        />
        <property 
            name="totalprice"
            column="TOTALPRICE"
            update="true"
            insert="true"
            type="java.lang.Double"
            not-null="true"
            unique="false"
            length="200"
        />
        <property 
            name="confirmtime"
            column="CONFIRMTIME"
            update="true"
            insert="true"
            type="java.util.Date"
            not-null="true"
            unique="false"
            length="0"
        />
        <property 
            name="paytime"
            column="PAYTIME"
            update="true"
            insert="true"
            type="java.util.Date"
            not-null="false"
            unique="false"
            length="0"
        />
        <property 
            name="orderstatus"
            column="ORDERSTATUS"
            update="true"
            insert="true"
            type="java.lang.Integer"
            not-null="true"
            unique="false"
            length="0"
        />
        <property 
            name="rsv2"
            column="RSV2"
            update="true"
            insert="true"
            type="java.lang.String"
            not-null="false"
            unique="false"
            length="40"
        />
		<!-- One-To-Many attributes -->
		<!-- Many-To-One attributes -->
    </class>
</hibernate-mapping>

